    dcterms:reviewed "31/10/2022"^^<xsd:date> ;
    SIO:SIO_000147 obo:ENVO_01000739, obo:GO_0040007, <http://vocabs.lter-europe.net/EnvThes/10107>, <http://vocabs.lter-europe.net/EnvThes/21785>, <http://vocabs.lter-europe.net/EnvThes/21880>, EFO:EFO_0005061 ;
    ets:valueType obo:STATO_0000252 ;
    skos:altLabel "plant_growth_substrate" ;
    skos:broader APD:trait_group_0030001 ;
    skos:definition "Trait broadly indicating where a plant is usually found, referencing the habitat in which a plant grows, the substrate to which a plant is attached, or a plant's growth medium."@en ;
    skos:example "close match:  aquatic_1 [GIFT:1.7.1] (https://gift.uni-goettingen.de)", "close match: epiphyte_2 [GIFT:1.3.2] (https://gift.uni-goettingen.de)", "related match: Plant growth form [TRY:42] (https://www.try-db.org/de/de.php)" ;
