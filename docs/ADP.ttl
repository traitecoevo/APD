    ets:minAllowedValue "0.5" ;
    ets:valueType obo:STATO_0000251 ;
    a owl:Class, skos:Concept ;
    rdfs:label "Root biomass depth distribution coefficient"@en ;
    rdfs:subClassOf APD:trait_0012104 ;
    skos:altLabel "root_distribution_coefficient" ;
    skos:broader APD:trait_0012104 ;
    skos:closeMatch "Root evenness of dry mass vertical distribution [TRY:3033] (https://www.try-db.org/de/de.php)" ;
    skos:inScheme "https://github.com/traitecoevo/APD" ;
    skos:prefLabel "Root biomass depth distribution coefficient"@en ;
    ont:hasContextObject obo:PO_0009005 ;
    uom:UCUM_code "{dimensionless}" .

