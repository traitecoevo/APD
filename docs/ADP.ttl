    rdfs:label "lamina"@en ;
    skos:inScheme "http://purl.obolibrary.org/obo/" .

obo:PO_0025073 a owl:Class ;
    dc:identifier "PO_0025073" ;
    dcterms:description "A shoot axis (PO:00025029) that develops from an axillary bud meristem (PO:0000232) or from equal divisions of a meristematic apical cell (PO:0030007)."@en ;
