    skos:inScheme "https://github.com/traitecoevo/APD" ;
    skos:narrower <https://github.com/traitecoevo/woodiness_detailed_herbaceous>, <https://github.com/traitecoevo/woodiness_detailed_semi_woody>, <https://github.com/traitecoevo/woodiness_detailed_woody>, <https://github.com/traitecoevo/woodiness_detailed_woody_base>, <https://github.com/traitecoevo/woodiness_detailed_woody_like_inflorescence>, <https://github.com/traitecoevo/woodiness_detailed_woody_like_stem>, <https://github.com/traitecoevo/woodiness_detailed_woody_root> ;
    skos:prefLabel "Detailed woodiness categories"@en ;
    skos:scopeNote "Vascular plants"@en ;
