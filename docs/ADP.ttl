    rdfs:label "strobilus"@en ;
    skos:inScheme "http://purl.obolibrary.org/obo/" .

obo:PO_0025117 a owl:Class ;
    dc:identifier "PO_0025117" ;
    dcterms:description "An anatomical space that is part of a plant. CARO:0000005 'anatomical space' is defined as: An immaterial anatomical entity of three dimensions, that is generated by morphogenetic or other physiologic processes, and is bounded by an anatomical surface."@en ;
    rdfs:label "plant anatomical space; lumen"@en ;
