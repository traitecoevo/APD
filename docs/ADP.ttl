    skos:inScheme "https://github.com/traitecoevo/APD" ;
    skos:narrower APD:life_history_annual, APD:life_history_biennial, APD:life_history_ephemeral, APD:life_history_perennial, APD:life_history_short_lived_perennial ;
    skos:prefLabel "Life history"@en ;
