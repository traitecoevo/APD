    ets:valueType obo:STATO_0000252 ;
    a owl:Class, skos:Concept ;
    rdfs:comment "This trait is narrowly applicable only to taxa that experience fire; see the trait `fire_exposure_level` for trait values related to species whose strategy is to avoid fire."@en ;
    rdfs:label "Fire response"@en ;
    rdfs:subClassOf APD:trait_0030602 ;
    skos:altLabel "fire_response" ;
    skos:broader APD:trait_0030602 ;
    skos:exactMatch "Resprouting capacity after fire [BROT:34] (http://doi.org/10.1038/sdata.2018.135)", "Species tolerance to fire [TRY:318]; Plant resprouting capacity [TRY:819] (https://www.try-db.org/de/de.php)" ;
    skos:inScheme "https://github.com/traitecoevo/APD" ;
    skos:narrower APD:fire_response_fire_killed, APD:fire_response_no_significant_fire_response, APD:fire_response_partial_resprouting, APD:fire_response_resprouts, APD:fire_response_unknown ;
    skos:prefLabel "Fire response"@en ;
    skos:scopeNote "applicable to plants that experience fire"@en ;
    ont:hasContextObject obo:PO_0000003, obo:PO_0000055 .

